{
  "name": "farmatime-client",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start-dev": "yarn inject && vite --host",
    "start": "vite preview",
    "build": "scripts/build-if-changes.sh",
    "build:production": "NODE_ENV=production yarn build",
    "build:dev": "NODE_ENV=development yarn build",
    "build:staging": "NODE_ENV=staging yarn build",
    "build-force": "echo \"Mode is $NODE_ENV\" && yarn build-prepare && yarn build-execute && yarn build-finalize",
    "build-prepare": "yarn inject && yarn i18n-check && yarn eslint-check && yarn build-info-produce && yarn index-produce && yarn sitemap-produce && yarn robots-produce && yarn favicons-produce && yarn manifest-produce && rm -rf build/",
    "build-execute": "KEEP_CONSOLE=true KEEP_DEBUGGER=true vite build --mode $NODE_ENV",
    "build-finalize": "echo 'Copying build to server' && rm -rf ../farmatime-server/client/build; cp -a build ../farmatime-server/client/build",
    "inject": "echo 'Injecting config...' && cd ../farmatime-server && yarn inject && cd ../farmatime-client",
    "build-info-produce": "/usr/bin/echo -n 'Producing build-info...' && node --no-warnings=ExperimentalWarning scripts/build-info-produce.js",
    "eslint-check": "eslint src/*.js src/*.jsx src/components/ src/hooks src/libs src/middlewares src/providers src/themes 2>&1",
    "index-produce": "echo 'Producing index...' && node --no-warnings=ExperimentalWarning scripts/index-produce.js",
    "sitemap-produce": "echo 'Producing sitemap...' && node --no-warnings=ExperimentalWarning scripts/sitemap-produce.js",
    "robots-produce": "echo 'Producing robots...' && node --no-warnings=ExperimentalWarning scripts/robots-produce.js",
    "favicons-produce": "echo 'Producing favicons...' && scripts/favicons-produce.sh",
    "manifest-produce": "echo 'Producing manifest...' && node --no-warnings=ExperimentalWarning scripts/manifest-produce",
    "i18n-prepare": "bash -c 'for dir in $(cat ./src/config.json | jq -r \".locales | keys[]\"); do mkdir -p \"src/locales/$dir/\"; done'",
    "i18n": "yarn i18n-prepare; i18next 'src/**/*.{js,jsx}' --config 'i18next-parser.config.cjs' --silent",
    "i18n-check": "sh -c 'yarn i18n && missing=$(grep --only-matching __STRING_NOT_TRANSLATED__ src/locales/*/translation.json | wc -l); if [ $missing -gt 0 ]; then echo \"$missing strings have to be translated!\"; exit 1; fi'",
    "i18n-translate": "node scripts/translate.mjs",
    "download-language-flags": "node scripts/download-language-flags.js",
    "lighthouse": "lighthouse http://localhost:5005/ --output json --output-path test/lighthouseOutput.json",
    "browserlist-update": "npx browserslist@latest --update-db",
    "deps-check": "npx unimported > /dev/null",
    "checks": "yarn i18n-check && yarn deps-check && yarn eslint-check && echo \"All checks passed\"",
    "update-medicines": "node scripts/download-AIFA-data.cjs"
  },
  "dependencies": {
    "@date-io/date-fns": "^3.2.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@lexical/html": "^0.32.1",
    "@lexical/react": "^0.32.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/x-date-pickers": "^7.2.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@yudiel/react-qr-scanner": "^2.3.1",
    "axios": "^1.7.7",
    "date-fns": "^3.2.0",
    "draftjs-to-html": "^0.9.1",
    "framer-motion": "^12.23.0",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "leaflet": "^1.9.4",
    "lexical": "^0.32.1",
    "libphonenumber-js": "^1.11.7",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lucide-react": "^0.515.0",
    "mui-tel-input": "^9.0.1",
    "notistack": "^3.0.1",
    "prop-types": "^15.8.1",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-gravatar": "^2.6.3",
    "react-i18next": "^15.5.2",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.26.1",
    "react-swipeable": "^7.0.2",
    "react-use": "^17.6.0",
    "swiper": "^11.2.10"
